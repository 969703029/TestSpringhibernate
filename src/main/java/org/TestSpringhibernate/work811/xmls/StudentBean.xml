<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    <hibernate-mapping>
    	<class name="org.TestSpringhibernate.work811.bens.StudentBean" table="t_student" catalog="kaoshi1">
    		<id name="id" column="pk_stuId" type="java.lang.Long"><generator class="identity"></generator> </id>
